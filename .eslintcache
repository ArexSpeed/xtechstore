= 'Tablet')\n  .map((tablet, index) => (\n    <ShopBox key={index}>\n              <ShopBoxImg src={tablet.img} />\n              <ShopBoxName>{tablet.model}</ShopBoxName>\n              <ShopBoxDesc>{tablet.ram}/{tablet.storage}/{tablet.size}\"</ShopBoxDesc>\n              <ShopBoxActions>\n                <ActionsPrice>{tablet.price}$</ActionsPrice>\n                <ActionsAdd onClick={() => addToBag(tablet)}>+</ActionsAdd>\n                <Link to={`/product/${tablet.id}`}> <ActionsDetails>Details</ActionsDetails></Link>\n              </ShopBoxActions>\n            </ShopBox>\n  ))\n\n  const showWatchesBox = products.filter(product => product.device === 'Watch')\n  .map((watch, index) => (\n    <ShopBox key={index}>\n              <ShopBoxImg src={watch.img} />\n              <ShopBoxName>{watch.model}</ShopBoxName>\n              <ShopBoxDesc>{watch.ram}/{watch.storage}/{watch.size}\"</ShopBoxDesc>\n              <ShopBoxActions>\n                <ActionsPrice>{watch.price}$</ActionsPrice>\n                <ActionsAdd onClick={() => addToBag(watch)}>+</ActionsAdd>\n                <Link to={`/product/${watch.id}`}> <ActionsDetails>Details</ActionsDetails></Link>\n              </ShopBoxActions>\n            </ShopBox>\n  ))\n\n  const showAccessoriesBox = products.filter(product => product.device === 'Accessory')\n  .map((accessory, index) => (\n    <ShopBox key={index}>\n              <ShopBoxImg src={accessory.img} />\n              <ShopBoxName>{accessory.model}</ShopBoxName>\n\n              <ShopBoxActions>\n                <ActionsPrice>{accessory.price}$</ActionsPrice>\n                <ActionsAdd onClick={() => addToBag(accessory)}>+</ActionsAdd>\n                <Link to={`/product/${accessory.id}`}> <ActionsDetails>Details</ActionsDetails></Link>\n              </ShopBoxActions>\n            </ShopBox>\n  ))\n  return (\n    <>\n      <ShopContainer>\n        <ShopBanner img={bg} alt=''>\n          \n        </ShopBanner>\n\n        <ShopSection>\n        <ShopBoxSection id=\"phones\">\n          <ShopBoxTitle>\n            <h3>Phones</h3>{\" \"}\n          </ShopBoxTitle>\n          <ShopBoxes>\n            {showPhoneBox}\n          </ShopBoxes>\n        </ShopBoxSection>\n        <ShopBoxSection id=\"ultrabooks\">\n          <ShopBoxTitle> <h3>Ultrabooks</h3></ShopBoxTitle>\n          <ShopBoxes>\n            {showXBooksBox}\n          </ShopBoxes>\n        </ShopBoxSection>\n        <ShopBoxSection id=\"watches\">\n          <ShopBoxTitle> <h3>Watches</h3></ShopBoxTitle>\n          <ShopBoxes>\n            {showWatchesBox}\n          </ShopBoxes>\n        </ShopBoxSection>\n        <ShopBoxSection id=\"tablets\">\n          <ShopBoxTitle> <h3>Tablets</h3></ShopBoxTitle>\n          <ShopBoxes>\n            {showTabletsBox}\n          </ShopBoxes>\n        </ShopBoxSection>\n        <ShopBoxSection id=\"accessories\">\n          <ShopBoxTitle> <h3>Accessories</h3></ShopBoxTitle>\n          <ShopBoxes>\n            {showAccessoriesBox}\n          </ShopBoxes>\n        </ShopBoxSection>\n        </ShopSection>\n      </ShopContainer>\n    </>\n  );\n}\n\nexport default Shop;\n","/Users/arek/Desktop/projects/xtech/src/pages/product.js",[],"/Users/arek/Desktop/projects/xtech/src/components/BagModal/index.js",["303","304","305"],"/Users/arek/Desktop/projects/xtech/src/components/BagModal/BagStyled.js",["306"],"import styled from \"styled-components\";\nimport * as style from \"../StyleVariables\";\n\nexport const BagContainer = styled.section`\n  position: fixed;\n  top: 80px;\n  right: 10%;\n  width: 350px;\n  max-height: 400px;\n  background: #fff;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  align-items: center;\n  border-radius: 20px;\n  border: none;\n  background: #fff;\n  box-shadow: 5px 0 10px rgba(0, 0, 0, 0.2),\n    -5px 0 10px rgba(255, 255, 255, 0.2);\n  z-index: 10;\n  overflow-y: scroll;\n`;\nexport const BagTitle = styled.h3`\n  width: 100%;\n  text-align: center;\n  font-size: 20px;\n`;\n\nexport const BagItem = styled.div`\n  width: 100%;\n  height: 80px;\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  padding: 10px;\n  &:hover {\n    box-shadow: 0 2px 2px rgba(0, 0, 0, 0.1), 0 -2px 2px rgba(0, 0, 0, 0.1);\n  }\n`;\nexport const ItemImg = styled.img`\n  max-height: 70px;\n`;\nexport const ItemDetails = styled.div`\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n  align-items: center;\n  min-width: 70%;\n`;\nexport const ItemName = styled.h5`\n  color: #000;\n  font-size: 22px;\n`;\nexport const ItemPrice = styled.h6`\n  color: #999;\n  font-size: 20px;\n`;\nexport const ItemQty = styled.div`\n  display: flex;\n  flex-direction: row;\n  font-size: 18px;\n  background: transparent;\n`;\nexport const QtyMinus = styled.button`\n  color: #fff;\n  background: red;\n  margin: 0 5px;\n  border: none;\n  height: 20px;\n  width: 20px;\n  border-radius: 50%;\n  border: none;\n`;\nexport const QtyPlus = styled.button`\n  color: #fff;\n  background: green;\n  margin: 0 5px;\n  border: none;\n  height: 20px;\n  width: 20px;\n  border-radius: 50%;\n  border: none;\n`;\nexport const QtySum = styled.button`\n  color: #000;\n  margin: 0 5px;\n  border: none;\n  background: transparent;\n`;\n\nexport const ItemDelete = styled.button`\n  color: red;\n  font-size: 20px;\n  border: none;\n  background: transparent;\n`;\n","/Users/arek/Desktop/projects/xtech/src/components/ProductSite/ProductStyled.js",[],"/Users/arek/Desktop/projects/xtech/src/components/ProductSite/index.js",["307"],"import React, {useState, useEffect, useContext} from \"react\";\nimport { withRouter, Link } from 'react-router-dom';\nimport {products} from '../data';\nimport {StateContext} from '../../StateProvider'\nimport {actionTypes} from '../../reducer'\n\nimport {\n  ProductContainer,\n  ProductDetails,\n  ProductImage,\n  ProductContainerImage,\n  ProductImageTitle,\n  ProductTitle,\n  ProductPrice,\n  ProductDesc,\n  ProductTable,\n  ProductTableRow,\n  ProductTr,\n  ProductTd,\n  ProductTdDesc,\n  ProductButton,\n  ProductOther,\n  ProductOtherTitle,\n\n} from \"./ProductStyled\";\n\nimport {\n  ShopBoxes,\n  ShopBox,\n  ShopBoxImg,\n  ShopBoxName,\n  ShopBoxDesc,\n  ShopBoxActions,\n  ActionsPrice,\n  ActionsAdd,\n  ActionsDetails,\n} from \"../Shop/ShopStyled\";\n\nconst ProductSite = ({match}) => {\n  const [device, setDevice] = useState()\n  const [{bagItems}, dispatch] = useContext(StateContext);\n  useEffect(() => {\n    window.scrollTo(0,0);\n  }, [])\n  useEffect(() => {\n    products.find(product => product.id === +match.params.id && setDevice(product.device))\n    console.log(device, 'device')\n  }, [match.params])\n\n\n  const addToBag = (phone) => {\n\n    dispatch({type: actionTypes.ADD_BAG_ITEM, bagItems: [...bagItems, {...phone, qty: 1}]})\n  }\n \n  const showProduct = products.filter(product => product.id === +match.params.id)\n  .map((product, index) => (\n    product.device === 'Phone' || product.device === 'Ultrabook' || product.device === 'Tablet'\n    ?\n    (\n    <>\n    <ProductContainerImage key={index}>\n    <ProductImageTitle>{product.series}</ProductImageTitle>\n    <ProductImage src={product.img} />\n  </ProductContainerImage>\n\n  <ProductDetails>\n    <ProductTitle>{product.model}</ProductTitle>\n    <ProductPrice>{product.price}$</ProductPrice>\n    <ProductDesc>{product.description}</ProductDesc>\n    <ProductTable>\n      <ProductTableRow>\n      <ProductTr>\n        <ProductTd>{product.ram}</ProductTd>\n        <ProductTdDesc>RAM</ProductTdDesc>\n      </ProductTr>\n      <ProductTr>\n        <ProductTd>{product.storage}</ProductTd>\n        <ProductTdDesc>Storage</ProductTdDesc>\n      </ProductTr>\n      <ProductTr>\n        <ProductTd>{product.size}\"</ProductTd>\n        <ProductTdDesc>Size</ProductTdDesc>\n      </ProductTr>\n      </ProductTableRow>\n      <ProductTableRow>\n      <ProductTr>\n        <ProductTd>{product.camera}</ProductTd>\n        <ProductTdDesc>Camera</ProductTdDesc>\n      </ProductTr>\n      <ProductTr>\n        <ProductTd>{product.cpu}</ProductTd>\n        <ProductTdDesc>CPU</ProductTdDesc>\n      </ProductTr>\n      <ProductTr>\n        <ProductTd>{product.battery}</ProductTd>\n        <ProductTdDesc>Battery</ProductTdDesc>\n      </ProductTr>\n      </ProductTableRow>\n    </ProductTable>\n\n    <ProductButton onClick={() => addToBag(product)}>Add to cart</ProductButton>\n  </ProductDetails>\n  </>\n    )\n    : product.device === 'Watch' ?\n\n    (\n      <>\n      <ProductContainerImage key={index}>\n      <ProductImageTitle>{product.series}</ProductImageTitle>\n      <ProductImage src={product.img} />\n    </ProductContainerImage>\n  \n    <ProductDetails>\n      <ProductTitle>{product.model}</ProductTitle>\n      <ProductPrice>{product.price}$</ProductPrice>\n      <ProductDesc>{product.description}</ProductDesc>\n      <ProductTable>\n        <ProductTableRow>\n        <ProductTr>\n          <ProductTd>{product.ram}</ProductTd>\n          <ProductTdDesc>RAM</ProductTdDesc>\n        </ProductTr>\n        <ProductTr>\n          <ProductTd>{product.storage}</ProductTd>\n          <ProductTdDesc>Storage</ProductTdDesc>\n        </ProductTr>\n        <ProductTr>\n          <ProductTd>{product.size}\"</ProductTd>\n          <ProductTdDesc>Size</ProductTdDesc>\n        </ProductTr>\n        </ProductTableRow>\n        <ProductTableRow>\n        <ProductTr>\n          <ProductTd>{product.fitness}</ProductTd>\n          <ProductTdDesc>Sports</ProductTdDesc>\n        </ProductTr>\n        <ProductTr>\n          <ProductTd>{product.cpu}</ProductTd>\n          <ProductTdDesc>CPU</ProductTdDesc>\n        </ProductTr>\n        <ProductTr>\n          <ProductTd>{product.battery}</ProductTd>\n          <ProductTdDesc>Battery</ProductTdDesc>\n        </ProductTr>\n        </ProductTableRow>\n        <ProductTableRow>\n        <p>Gps: {product.addons.gps ? <span>&#10003;</span> : \t<span>&times;</span>}</p>\n      <p>Health Care: {product.addons.healthcare ? <span>&#10003;</span> : <span>&times;</span>}</p>\n      <p>WiFi: {product.addons.wifi ? <span>&#10003;</span> : <span>&times;</span>}</p>\n      <p>Esim: {product.addons.esim ? <span>&#10003;</span> : <span>&times;</span>}</p>\n      <p>Waterproof: {product.addons.waterproof ? <span>&#10003;</span> : <span>&times;</span>}</p>\n        </ProductTableRow>\n      </ProductTable>\n  \n      <ProductButton onClick={() => addToBag(product)}>Add to cart</ProductButton>\n    </ProductDetails>\n    </>\n      ) : \n      product.device === 'Accessory' ?\n\n    (\n      <>\n      <ProductContainerImage key={index}>\n      <ProductImageTitle>{product.series}</ProductImageTitle>\n      <ProductImage src={product.img} />\n    </ProductContainerImage>\n  \n    <ProductDetails>\n      <ProductTitle>{product.model}</ProductTitle>\n      <ProductPrice>{product.price}$</ProductPrice>\n      <ProductDesc>{product.description}</ProductDesc>\n      \n  \n      <ProductButton onClick={() => addToBag(product)}>Add to cart</ProductButton>\n    </ProductDetails>\n    </>\n      ) : ''\n  ))\n\n  \n  const showOther = products.filter(product => product.device === device)\n  .map((product, index) => (\n    \n    <ShopBox key={index}>\n              <ShopBoxImg src={product.img} />\n              <ShopBoxName>{product.model}</ShopBoxName>\n              {product.device !== 'Accessory' && (<ShopBoxDesc>{product.ram}/{product.storage}/{product.size}\"</ShopBoxDesc>)}\n              \n              <ShopBoxActions>\n                <ActionsPrice>{product.price}$</ActionsPrice>\n                <ActionsAdd onClick={() => addToBag(product)}>+</ActionsAdd>\n                <Link to={`/product/${product.id}`} onClick={() => window.scrollTo(0,0)}> <ActionsDetails>Details</ActionsDetails></Link>\n              </ShopBoxActions>\n            </ShopBox>\n    \n  ))\n\n  return (\n    <>\n      <ProductContainer>\n          {showProduct}\n      </ProductContainer>\n      \n      <ProductOther>\n        <ProductOtherTitle>Other {device}</ProductOtherTitle>\n        <ShopBoxes>\n          {showOther}\n        </ShopBoxes>\n      </ProductOther>\n    </>\n  );\n}\n\nexport default withRouter(ProductSite);\n",["308","309"],"/Users/arek/Desktop/projects/xtech/src/reducer.js",[],"/Users/arek/Desktop/projects/xtech/src/StateProvider.js",["310","311"],"import React, { createContext, useState, useContext, useReducer } from \"react\";\nimport reducer, {initialState} from './reducer';\n\nexport const StateContext = createContext();\n\nexport const StateProvider = ({children}) => (\n  <StateContext.Provider value={useReducer(reducer, initialState)}>\n  {children}\n</StateContext.Provider>\n)\n\n","/Users/arek/Desktop/projects/xtech/src/pages/admin.js",[],"/Users/arek/Desktop/projects/xtech/src/components/AdminMain/AddItem.js",[],["312","313"],"/Users/arek/Desktop/projects/xtech/src/components/AdminMain/AdminStyled.js",[],"/Users/arek/Desktop/projects/xtech/src/components/AdminMain/index.js",["314","315","316"],"/Users/arek/Desktop/projects/xtech/src/components/AdminMain/EditItem.js",["317","318"],"import React, {useState, useEffect, useContext} from 'react'\nimport FileBase from 'react-file-base64'\nimport axios from 'axios'\nimport {\n  AddItemContainer,FormAdd,FormRow, FormInput, FormLabel, FormButton\n} from \"./AdminStyled\";\nimport {StateContext} from '../../StateProvider'\nimport {actionTypes} from '../../reducer'\n\n\nconst EditItem = ({edit}) => {\n  const [{adminPhones,currentEditId}, dispatch] = useContext(StateContext)\n  const [item, setItem] = useState([])\n\nconst currentItemId = currentEditId\n\nuseEffect(() => {\n  const selectedItem = adminPhones.find(item => item._id === currentItemId)\n  if(selectedItem){\n    setItem(selectedItem)\n  }\n},[adminPhones, currentItemId])\n\n  const sendItem = () => {\n    axios.put(`/api/phones/${currentItemId}`, item)\n  }\n\n\n  return (\n    <>\n    {edit &&\n      <AddItemContainer>\n        <FormAdd onSubmit={sendItem}>\n          <FormRow>\n            <FormLabel htmlFor=\"series\">Series:</FormLabel>\n            <FormInput\n              name=\"series\"\n              type=\"text\"\n              fullWith\n              value={item.series}\n              onChange={(e) => setItem({ ...item, series: e.target.value })}\n            />\n          </FormRow>\n          <FormRow>\n            <FormLabel htmlFor=\"model\">Model:</FormLabel>\n            <FormInput\n              name=\"model\"\n              type=\"text\"\n              fullWith\n              value={item.model}\n              onChange={(e) => setItem({ ...item, model: e.target.value })}\n            />\n          </FormRow>\n\n          <FormRow>\n            <FormLabel htmlFor=\"description\">Description:</FormLabel>\n            <FormInput\n              name=\"description\"\n              type=\"text\"\n              fullWith\n              value={item.description}\n              onChange={(e) => setItem({ ...item, description: e.target.value })}\n            />\n          </FormRow>\n\n          <FormRow>\n            <FormLabel htmlFor=\"cpu\">CPU:</FormLabel>\n            <FormInput\n              name=\"cpu\"\n              type=\"text\"\n              fullWith\n              value={item.cpu}\n              onChange={(e) => setItem({ ...item, cpu: e.target.value })}\n            />\n          </FormRow>\n\n          <FormRow>\n            <FormLabel htmlFor=\"ram\">Ram:</FormLabel>\n            <FormInput\n              name=\"ram\"\n              type=\"text\"\n              fullWith\n              value={item.ram}\n              onChange={(e) => setItem({ ...item, ram: e.target.value })}\n            />\n          </FormRow>\n\n          <FormRow>\n            <FormLabel htmlFor=\"storage\">Storage:</FormLabel>\n            <FormInput\n              name=\"storage\"\n              type=\"text\"\n              fullWith\n   