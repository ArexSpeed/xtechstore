ainer, Content, H1, P, Ul, Li, PhoneBoxes, PhonesShow, PhoneBox, PhoneImage, PhoneName, SectionDesc, DescDetails, DescImage, DescPhoneName, Table, Tr, Td, Button} from './PhonesStyled';\nimport {products} from '../data';\n\nfunction SectionPhones() {\n  const [showPhoneSeries, setShowPhoneSeries] = useState('Pro Series');\n  const [showPhoneModel, setShowPhoneModel] = useState('Pro 21 Max');\n  const [boxClass, setBoxClass] = useState(false);\n  const handleShowPhoneSeries = (e) => {\n    let seriesName = e.target.textContent;\n    setShowPhoneSeries(seriesName);\n    console.log(seriesName.split(' ').slice(0,1));\n    let modelName = seriesName.split(' ').slice(0,1);\n\n    if(modelName[0] === 'Game') modelName[0] = 'G'\n    setShowPhoneModel(`${modelName} 21`);\n  }\n  useEffect(\n    () => {\n      setBoxClass(!boxClass);\n    },\n    [showPhoneModel],\n  );\n\n\n  //with error on image click\n  //const handleShowPhoneModelE = (e) => setShowPhoneModel(e.target.lastChild.textContent)\n  //Set model to rotate details\n  const handleShowPhoneModel = (model) => {\n    setShowPhoneModel(model)\n  }\n\n  const showBoxes =  products.filter(product => product.device === 'Phone')\n  .filter(phone => phone.series === showPhoneSeries)\n  .map((phone, index) =>  (\n              <PhoneBox key={index} onClick={() => handleShowPhoneModel(phone.model)}>\n                <PhoneImage src={phone.img} />\n\n                <PhoneName>\n                  {phone.model}\n                </PhoneName>\n              </PhoneBox>\n\n      ))\n\n  const showPhoneModelBox =  products.filter(product => product.device === 'Phone')\n  .filter(phone => phone.model === showPhoneModel)\n  .map((phone, index) => (\n    <>\n    <DescImage src={phone.img} />\n    <DescDetails \n     className={boxClass ? 'rotate' : 'rotate2'}\n    >\n    <DescPhoneName>{phone.model}</DescPhoneName>\n    <Table>\n      <Tr>\n        <Td>CPU:</Td>\n        <Td>{phone.cpu}</Td>\n      </Tr>\n\n      <Tr>\n        <Td>RAM:</Td>\n        <Td>{phone.ram}</Td>\n      </Tr>\n\n      <Tr>\n        <Td>Storage:</Td>\n        <Td>{phone.storage}</Td>\n      </Tr>\n      <Tr>\n        <Td>Camera:</Td>\n        <Td>{phone.camera}</Td>\n      </Tr>\n      <Tr>\n        <Td>Size:</Td>\n        <Td>{phone.size}</Td>\n      </Tr>\n      <Tr>\n        <Td>Battery:</Td>\n        <Td>{phone.battery}</Td>\n      </Tr>\n      <Tr>\n        <Td>Price:</Td>\n        <Td>{phone.price}</Td>\n      </Tr>\n    </Table>\n    <Link to={`/product/${phone.id}`}>\n    <Button>\n      Go to product\n    </Button>\n    </Link>\n    </DescDetails>\n    </>\n  ))\n  \n  return (\n\n <>\n<Container id='phones'>\n   <Content>\n        <H1>Smartphones for everyone</H1>\n        <P>\n          Check our smartphones from budget level if you need basic usage to most advanced devices in the market\n        </P>\n      <Ul>\n        <Li onClick={handleShowPhoneSeries} className={showPhoneSeries=== 'Pro Series' ? 'active' : ''}>\n          Pro Series\n        </Li>\n        <Li onClick={handleShowPhoneSeries} className={showPhoneSeries=== 'A Series' ? 'active' : ''}>\n          A Series\n        </Li>\n        <Li onClick={handleShowPhoneSeries} className={showPhoneSeries=== 'Game Series' ? 'active' : ''}>\n          Game Series\n        </Li>\n        <Li onClick={handleShowPhoneSeries} className={showPhoneSeries=== 'Lite Series' ? 'active' : ''}>\n          Lite Series\n        </Li>\n      </Ul>\n      <PhonesShow>\n        <PhoneBoxes>\n        {showBoxes}\n        </PhoneBoxes>\n        \n      <SectionDesc>\n      {showPhoneModelBox}\n              \n      </SectionDesc>\n      </PhonesShow>\n      </Content>\n      </Container>\n\n</>\n\n  )\n}\n\nexport default SectionPhones;","/Users/arek/Desktop/projects/xtech/src/components/SectionPhones/PhonesStyled.js",[],"/Users/arek/Desktop/projects/xtech/src/components/SectionBooks/XBookStyled.js",["298","299","300"],"/Users/arek/Desktop/projects/xtech/src/components/SectionBooks/index.js",["301"],"/Users/arek/Desktop/projects/xtech/src/components/ShopNavbar/index.js",[],"/Users/arek/Desktop/projects/xtech/src/components/Shop/ShopStyled.js",[],"/Users/arek/Desktop/projects/xtech/src/components/Shop/index.js",["302"],"import React, {useContext} from \"react\";\nimport {Link} from 'react-router-dom';\nimport { WaveDown } from \"../SectionWave\";\nimport {products} from '../data';\nimport bg from '../../images/shopbg.png'\n\nimport {\n  ShopContainer,\n  ShopBanner,\n  ShopSection,\n  ShopBoxSection,\n  ShopBoxTitle,\n  ShopBoxes,\n  ShopBox,\n  ShopBoxImg,\n  ShopBoxName,\n  ShopBoxDesc,\n  ShopBoxActions,\n  ActionsAdd,\n  ActionsPrice,\n  ActionsDetails,\n} from \"./ShopStyled\";\nimport {StateContext} from '../../StateProvider'\nimport {actionTypes} from '../../reducer'\n\nfunction Shop() {\n  const [{bagItems}, dispatch] = useContext(StateContext);\n\n  const addToBag = (phone) => {\n    //console.log(model, price)\n    console.log(phone, 'add to bag')\n    dispatch({type: actionTypes.ADD_BAG_ITEM, bagItems: [...bagItems, {...phone, qty: 1}]})\n  }\n\n  const showPhoneBox = products.filter(product => product.device === 'Phone')\n  .map((phone, index) => (\n    <ShopBox key={index}>\n              <ShopBoxImg src={phone.img} />\n              <ShopBoxName>{phone.model}</ShopBoxName>\n              <ShopBoxDesc>{phone.ram}/{phone.storage}/{phone.size}\"</ShopBoxDesc>\n              <ShopBoxActions>\n                <ActionsPrice>{phone.price}$</ActionsPrice>\n                <ActionsAdd onClick={() => addToBag(phone)}>+</ActionsAdd>\n                <Link to={`/product/${phone.id}`}> <ActionsDetails>Details</ActionsDetails></Link>\n              </ShopBoxActions>\n            </ShopBox>\n  ))\n\n  const showXBooksBox = products.filter(product => product.device === 'Ultrabook')\n  .map((ultrabook, index) => (\n    <ShopBox key={index}>\n              <ShopBoxImg src={ultrabook.img} />\n              <ShopBoxName>{ultrabook.model}</ShopBoxName>\n              <ShopBoxDesc>{ultrabook.ram}/{ultrabook.storage}/{ultrabook.size}\"</ShopBoxDesc>\n              <ShopBoxActions>\n                <ActionsPrice>{ultrabook.price}$</ActionsPrice>\n                <ActionsAdd onClick={() => addToBag(ultrabook)}>+</ActionsAdd>\n                <Link to={`/product/${ultrabook.id}`}> <ActionsDetails>Details</ActionsDetails></Link>\n              </ShopBoxActions>\n            </ShopBox>\n  ))\n\n  const showTabletsBox = products.filter(product => product.device === 'Tablet')\n  .map((tablet, index) => (\n    <ShopBox key={index}>\n              <ShopBoxImg src={tablet.img} />\n              <ShopBoxName>{tablet.model}</ShopBoxName>\n              <ShopBoxDesc>{tablet.ram}/{tablet.storage}/{tablet.size}\"</ShopBoxDesc>\n              <ShopBoxActions>\n                <ActionsPrice>{tablet.price}$</ActionsPrice>\n                <ActionsAdd onClick={() => addToBag(tablet)}>+</ActionsAdd>\n                <Link to={`/product/${tablet.id}`}> <ActionsDetails>Details</ActionsDetails></Link>\n              </ShopBoxActions>\n            </ShopBox>\n  ))\n\n  const showWatchesBox = products.filter(product => product.device === 'Watch')\n  .map((watch, index) => (\n    <ShopBox key={index}>\n              <ShopBoxImg src={watch.img} />\n              <ShopBoxName>{watch.model}</ShopBoxName>\n              <ShopBoxDesc>{watch.ram}/{watch.storage}/{watch.size}\"</ShopBoxDesc>\n              <ShopBoxActions>\n                <ActionsPrice>{watch.price}$</ActionsPrice>\n                <ActionsAdd onClick={() => addToBag(watch)}>+</ActionsAdd>\n                <Link to={`/product/${watch.id}`}> <ActionsDetails>Details</ActionsDetails></Link>\n              </ShopBoxActions>\n            </ShopBox>\n  ))\n\n  const showAccessoriesBox = products.filter(product => product.device === 'Accessory')\n  .map((accessory, index) => (\n    <ShopBox key={index}>\n              <ShopBoxImg src={accessory.img} />\n              <ShopBoxName>{accessory.model}</ShopBoxName>\n\n              <ShopBoxActions>\n                <ActionsPrice>{accessory.price}$</ActionsPrice>\n                <ActionsAdd onClick={() => addToBag(accessory)}>+</ActionsAdd>\n                <Link to={`/product/${accessory.id}`}> <ActionsDetails>Details</ActionsDetails></Link>\n              </ShopBoxActions>\n            </ShopBox>\n  ))\n  return (\n    <>\n      <ShopContainer>\n        <ShopBanner img={bg} alt=''>\n          \n        </ShopBanner>\n\n        <ShopSection>\n        <ShopBoxSection id=\"phones\">\n          <ShopBoxTitle>\n            <h3>Phones</h3>{\" \"}\n          </ShopBoxTitle>\n          <ShopBoxes>\n            {showPhoneBox}\n          </ShopBoxes>\n        </ShopBoxSection>\n        <ShopBoxSection id=\"ultrabooks\">\n          <ShopBoxTitle> <h3>Ultrabooks</h3></ShopBoxTitle>\n          <ShopBoxes>\n            {showXBooksBox}\n          </ShopBoxes>\n        </ShopBoxSection>\n        <ShopBoxSection id=\"watches\">\n          <ShopBoxTitle> <h3>Watches</h3></ShopBoxTitle>\n          <ShopBoxes>\n            {showWatchesBox}\n          </ShopBoxes>\n        </ShopBoxSection>\n        <ShopBoxSection id=\"tablets\">\n          <ShopBoxTitle> <h3>Tablets</h3></ShopBoxTitle>\n          <ShopBoxes>\n            {showTabletsBox}\n          </ShopBoxes>\n        </ShopBoxSection>\n        <ShopBoxSection id=\"accessories\">\n          <ShopBoxTitle> <h3>Accessories</h3></ShopBoxTitle>\n          <ShopBoxes>\n            {showAccessoriesBox}\n          </ShopBoxes>\n        </ShopBoxSection>\n        </ShopSection>\n      </ShopContainer>\n    </>\n  );\n}\n\nexport default Shop;\n","/Users/arek/Desktop/projects/xtech/src/pages/product.js",[],"/Users/arek/Desktop/projects/xtech/src/components/BagModal/index.js",["303","304","305"],"import React, { useState, useContext } from \"react\";\nimport {phones} from '../data';\n\nimport {\n  BagContainer,\n  BagTitle,\n  BagItem,\n  ItemImg,\n  ItemName,\n  ItemDelete,\n  ItemDetails,\n  ItemPrice,\n  ItemQty,\n  QtyMinus,\n  QtyPlus,\n  QtySum,\n} from \"./BagStyled\";\n\nimport {StateContext} from '../../StateProvider'\nimport {actionTypes} from '../../reducer'\n\n\nfunction BagModal({ open }) {\n  const [sum, setSum] = useState(0);\n  const [{bagItems}, dispatch] = useContext(StateContext)\n\n  // totalItems -> array of price and qty of one item in bag\n  let totalItems = []\n  const showBagItems = bagItems.map((item, index) => \n  {\n    totalItems.push({price: item.price, qty: item.qty})\n    return (\n      <BagItem key={index}>\n              <ItemImg src={item.img} />\n              <ItemDetails>\n                <ItemName>{item.model}</ItemName>\n                <ItemQty>\n                  <ItemPrice>${item.price}</ItemPrice>\n                  <QtyMinus onClick={() => item.qty > 1 ? dispatch({type: actionTypes.MINUS_ITEM_QTY, payload: item}) : dispatch({type: actionTypes.DELETE_BAG_ITEM, payload: item.id})}>-</QtyMinus>\n                  <QtySum>{item.qty}</QtySum>\n                  <QtyPlus onClick={() => dispatch({type: actionTypes.PLUS_ITEM_QTY, payload: item})}>+</QtyPlus>\n                </ItemQty>\n              </ItemDetails>\n              <ItemDelete onClick={() => dispatch({type: actionTypes.DELETE_BAG_ITEM, payload: item.id})}>X</ItemDelete>\n            </BagItem>\n    )\n  }\n  )\n  //totalPrice -> price of one chosen item -> price*qty\n  let totalPrice = []\n  totalItems.map(item => totalPrice.push(item.price*item.qty))\n  \n  return (\n    <>\n      {open && (\n        <BagContainer>\n          <BagTitle>My Cart</BagTitle>\n          Total: \n          ${bagItems.length > 0 && totalPrice.reduce((a,b) => a+b)}\n          {showBagItems}\n\n        </BagContainer>\n      )}\n    </>\n  );\n}\n\nexport default BagModal;\n","/Users/arek/Desktop/projects/xtech/src/components/BagModal/BagStyled.js",["306"],"import styled from \"styled-components\";\nimport * as style from \"../StyleVariables\";\n\nex